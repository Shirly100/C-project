<Window x:Class="PLWPF.removenanny"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF"
        mc:Ignorable="d"
        Title="removenanny" Height="237.333" Width="260.667">
    <Grid>
        <Label Content="Choose Nanny to Remove" HorizontalAlignment="Left" Margin="53,34,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="choose" HorizontalAlignment="Left" Margin="53,86,0,0" VerticalAlignment="Top" Width="152" Height="31" SelectionChanged="choose_SelectionChanged"/>
        <Button x:Name="remove" Content="Remove Nanny" HorizontalAlignment="Left" Margin="53,149,0,0" VerticalAlignment="Top" Width="152" Click="remove_Click"/>

    </Grid>
</Window>
