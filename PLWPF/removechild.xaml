<Window x:Class="PLWPF.removechild"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF"
        mc:Ignorable="d"
        Title="removechild" Height="300" Width="300">
    <Grid>
        <Label Content="Child ID:" HorizontalAlignment="Left" Margin="57,110,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="ID_c" HorizontalAlignment="Left" Margin="136,113,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="ID_c_SelectionChanged"/>
        <Button Content="Remove Child" HorizontalAlignment="Left" Margin="92,182,0,0" VerticalAlignment="Top" Width="126" Click="Button_Click" Height="41"/>

    </Grid>
</Window>
