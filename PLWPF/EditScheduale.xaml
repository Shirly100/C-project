        <TextBox x:Name="endThursday" HorizontalAlignment="Left" Height="23" Margin="7.333,9.333,0,0" TextWrapping="Wrap" Text="{Binding Path=mystuff[BE.Days.Thu].second}" VerticalAlignment="Top" Width="64" Grid.Row="7" Grid.Column="4" FontWeight="Bold" >
