        <TextBox x:Name="startTuesday" HorizontalAlignment="Left" Height="23" Margin="12,9.667,0,0" TextWrapping="Wrap" Text="{Binding Path=mystuff[BE.Days.Tue].first}" VerticalAlignment="Top" Width="64" Grid.Row="5" Grid.Column="3" FontWeight="Bold" >
