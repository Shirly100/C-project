                <DropShadowEffect BlurRadius="25"/>
            </TextBox.Effect>
        </TextBox>
        <TextBox x:Name="startMonday" HorizontalAlignment="Left" Height="23" Margin="12,9.333,0,0" TextWrapping="Wrap" Text="{Binding Path=mystuff[BE.Days.Mon].first}" VerticalAlignment="Top" Width="64" Grid.Row="4" Grid.Column="3" FontWeight="Bold" >
