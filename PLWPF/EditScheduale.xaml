        <CheckBox x:Name="checkMonday" Content="" HorizontalAlignment="Left" Margin="5.333,10.333,0,0" VerticalAlignment="Top" Grid.Row="4" Grid.Column="2" FontWeight="Bold" Height="15" Width="20" Grid.ColumnSpan="2">
