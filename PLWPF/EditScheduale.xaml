            </TextBox.Effect>
        </TextBox>
        <Button Content="Save Scheduale" HorizontalAlignment="Left" Margin="7,12.333,0,0" VerticalAlignment="Top" Width="249" Click="Button_Click_1" Grid.Row="8" Grid.Column="1" Grid.ColumnSpan="4" FontWeight="Bold" Height="20">
            <Button.Effect>
                <DropShadowEffect BlurRadius="25"/>
            </Button.Effect>
        </Button>
        <Grid.Background>
            <ImageBrush ImageSource="C:\Users\Chany\Desktop\school\Nanny\SuperManaging\PLWPF\Resources\90168532.jpg" Stretch="UniformToFill" Opacity="0.3"/>
        </Grid.Background>
