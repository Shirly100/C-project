        <TextBox x:Name="startWednesday" HorizontalAlignment="Left" Height="23" Margin="12,9.333,0,0" TextWrapping="Wrap" Text="{Binding Path=mystuff[BE.Days.Wed].first}" VerticalAlignment="Top" Width="64" Grid.Row="6" Grid.Column="3" FontWeight="Bold" >
