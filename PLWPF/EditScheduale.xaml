        </TextBox>
        <TextBox x:Name="endSunday" HorizontalAlignment="Left" Height="23" Margin="7.333,9.333,0,0" TextWrapping="Wrap" Text="{Binding mystuff[0].Value.Value}" VerticalAlignment="Top" Width="64" Grid.Row="3" Grid.Column="4" FontWeight="Bold" >
