        </CheckBox>
        <TextBox x:Name="startSunday" Text="{Binding mystuff[0].Value.Key}" HorizontalAlignment="Left" Height="23" Margin="12,9.333,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="64" Grid.Row="3" Grid.Column="3" FontWeight="Bold" >
            <TextBox.Effect>
