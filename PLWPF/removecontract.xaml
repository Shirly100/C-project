<Window x:Class="PLWPF.removecontract"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF"
        mc:Ignorable="d"
        Title="removecontract" Height="251" Width="246">
    <Grid>
        <Label Content="Choose Contract to Remove" HorizontalAlignment="Left" Margin="44,41,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="choose" HorizontalAlignment="Left" Margin="44,93,0,0" VerticalAlignment="Top" Width="152" Height="31" SelectionChanged="choose_SelectionChanged"/>
        <Button x:Name="remove" Content="Remove Contract" HorizontalAlignment="Left" Margin="44,156,0,0" VerticalAlignment="Top" Width="152" Click="remove_Click"/>

    </Grid>
</Window>
