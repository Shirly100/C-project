<Window x:Class="PLWPF.wnanny"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF"
        mc:Ignorable="d"
        Title="wnanny" Height="300" Width="300">
    <Grid>
        <Button Content="Add" HorizontalAlignment="Left" Margin="104,36,0,0" VerticalAlignment="Top" Width="75" Click="Add"/>
        <Button Content="Update" HorizontalAlignment="Left" Margin="104,95,0,0" VerticalAlignment="Top" Width="75" Click="Update"/>
        <Button Content="Remove" HorizontalAlignment="Left" Margin="104,154,0,0" VerticalAlignment="Top" Width="75" Click="Remove"/>
        <TextBox Name="tb" HorizontalAlignment="Left" Height="23" Margin="85,212,0,0" TextWrapping="Wrap" Text="Personal zone" VerticalAlignment="Top" Width="120" PreviewKeyDown="EnterClicked"/>
    </Grid>
</Window>
